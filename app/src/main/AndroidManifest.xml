<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.aabane.training">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.App">
        <activity
            android:name=".AddExerciseActivity"
            android:label="@string/title_activity_add_exercise"
            android:theme="@style/Theme.App.NoActionBar"></activity>
        <activity
            android:name=".TrainingExercises"
            android:label="@string/title_activity_training_exercises"
            android:theme="@style/Theme.App.NoActionBar" />

        <service
            android:name=".MyIntentService"
            android:exported="false" />

        <activity
            android:name=".Trainings"
            android:label="@string/title_activity_trainings"
            android:theme="@style/Theme.App.NoActionBar" />
        <activity
            android:name=".AddTraining"
            android:label="@string/title_activity_add_training"
            android:theme="@style/Theme.App.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>